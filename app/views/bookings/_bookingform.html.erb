<%= form_for @booking do |f|%>
<%= f.label :noticket%>
<%= f.text_field :noticket%>
<%= f.label :movie_id %>
<%= f.collection_select(:movie_id, @movies, :id, :name) %>
<%= f.label :user_id %>
<%= f.collection_select(:user_id, @users, :id, :name) %>
<%= f.submit 'submit'%>
<%end%>
<%= link_to 'back', bookings_url %>