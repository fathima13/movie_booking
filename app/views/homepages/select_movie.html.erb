<%= form_for @booking, url:"/theatre/#{params[:theatre]}/movies" do |f|%>
<%= f.label :movie_id %>
<%= f.collection_select(:movie_id, @movies, :id, :name) %>
<%=f.label :noticket %>
<%=f.text_field :noticket%>
<%=f.label :user_name%>
<%= f.text_field :user_name%>
<%= submit_tag 'submit' %>
<% end -%>